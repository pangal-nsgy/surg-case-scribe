{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
} 